<body>
    <%- include ./partials/navigation.html %>
        <div class = "container fluid">
        <h1>AFC</h1>
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>AFC North</th>
                    <th>W</th>
                    <th>L</th>
                    <th>T</th>
                    <th>Pct</th>
                    <th>PF</th>
                    <th>PA</th>
                    <th>Diff</th>
                </tr>
            </thead>
            <tbody>
              <% for(var i = 0; i < standings.length; i++) { %>
              <% if(standings[i].divisionName == 'AFC North') { %>
              <% var gamesPlayed = standings[i].totalWins + standings[i].totalLosses + standings[i].totalTies %>
              <% var pointsScored = standings[i].ptsFor * gamesPlayed %>
              <% var pointsAllowed = standings[i].ptsAgainst * gamesPlayed %>
                <tr>
                    <td class="col-md-3"><%=standings[i].teamName %></td>
                    <td class="col-md-1"><%=standings[i].totalWins %></td>
                    <td class="col-md-1"><%=standings[i].totalLosses %></td>
                    <td class="col-md-1"><%=standings[i].totalTies %></td>
                    <td class="col-md-1"><%=standings[i].winPct %></td>
                    <td class="col-md-1"><%=pointsScored %></td>
                    <td class="col-md-1"><%=pointsAllowed %></td>
                    <td class="col-md-1"><%=standings[i].netPts %></td>
